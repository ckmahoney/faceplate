{
  "name": "faceplate",
  "title": "Faceplate",
  "description": "A UI for playing generative music",
  "main": "npm run dev",
  "url": "https://tenpens.xyz",
  "target": "web",
  "private": false,
  "scripts": {
    "compile": "tsc && webpack --config dist.webpack.config.js",
    "build-local": "export NODE_ENV=local && npm run compile",
    "build-stage": "export NODE_ENV=stage && npm run compile",
    "dev": "export NODE_ENV=development && yarn serve",
    "serve": "webpack serve --config webpack.config.babel.js"
  },
